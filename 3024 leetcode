class Solution(object):
    def triangleType(self, nums):
        if len(set(nums))==1:
            return "equilateral"
        if nums[0]+nums[1]<=nums[2] or nums[1]+nums[2]<=nums[0] or nums[0]+nums[2]<=nums[1]:# điều kiện để là tam giác
            return "none"
        elif len(set(nums))==2:
            return "isosceles"
        else:
            return "scalene" 
