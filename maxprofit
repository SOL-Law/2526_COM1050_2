class Solution:
    def maxProfit(self, prices):
        gia_nho_nhat=prices[0]
        gia_lon_nhat_thu_dc =0
        for gia in prices:
            if gia<gia_nho_nhat:
                gia_nho_nhat=gia
            gia_tam_thoi_lon_nhat=gia-gia_nho_nhat
            if  gia_tam_thoi_lon_nhat>gia_lon_nhat_thu_dc :
                 gia_lon_nhat_thu_dc =gia_tam_thoi_lon_nhat
        return gia_lon_nhat_thu_dc


        

        
