class Solution:
    def countTriples(self, n: int) -> int:
        d=0
        for a in range(1,n+1): # duyệt từng phần tử và tránh 0
            for b in range(a+1,n+1):
                for c in range(b+1,n+1):
                    if a**2+b**2==c**2: 
                        d+=2  # có cặp (b,a) và (a,b)
        return d
